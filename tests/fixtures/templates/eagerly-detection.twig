{# Test fixture for Craft 5 .eagerly() detection #}

{# Good: Using .eagerly() for lazy eager loading #}
{% for entry in craft.entries.section('blog').all() %}
  {% set image = entry.featuredImage.eagerly().one() %}
  {{ image.url }}
{% endfor %}

{# Mixed strategy warning: Uses both .with() and .eagerly() #}
